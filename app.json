{
    "expo": {
        "name": "QuickAI",
        "slug": "quickai",
        "version": "1.3.4",
        "scheme": "quickai",
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            [
                "expo-router",
                {
                    "root": "./src/app"
                }
            ],
            [
                "expo-dev-launcher",
                {
                    "launchMode": "most-recent"
                }
            ],
            "expo-web-browser",
            "expo-localization"
        ],
        "experiments": {
            "typedRoutes": true,
            "tsconfigPaths": true
        },
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.example.quick"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.example.quick"
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "1956815e-b969-423e-b8fe-f0e210a07bb8"
            }
        }
    }
}
